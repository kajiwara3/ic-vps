<%= search_form_for([:admin, @search], html: {class: 'form-search'}, remote: true) do |f| %>
  <div class="field">
    <%= f.label :partner_name, 'パートナー' %>
    <%= f.text_field :partner_name_cont %>
  </div>
  <div class="field">
    <%= f.label :url, 'URL' %>
    <%= f.text_field :url_cont %>
  </div>
  <div class="field">
    <%= f.label :ip_address, 'IP' %>
    <%= f.text_field :ip_address_cont %>
  </div>
  <div class="field">
    <%= f.label :request_method, 'メソッド' %>
    <%= f.text_field :request_method_cont %>
  </div>
  <div class="field">
    <%= f.label :created_at_gteq, '日付' %>
    <%= f.datepicker 'created_at_gteq', class: "datepicker" %>
    <%= f.label :created_at_lteq, '〜' %>
    <%= f.datepicker 'created_at_lteq', class: "datepicker" %>
  </div>

  <div class="actions">
    <%= f.submit "検索する", class: "search-button" %>
  </div>
<% end %>
